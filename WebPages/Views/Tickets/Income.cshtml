@model Guid?

<h2>Validar Ticket de Ingreso</h2>

@if (Model.HasValue)
{
    <h4>Resultado de la validación:</h4>
    <p>@ViewData["Message"]</p>
}

<form method="post" action="/Tickets/ValidateIncomeTicket">
    <div class="form-group">
        <label for="ticketId">ID del ticket:</label>
        <input type="text" class="form-control" id="ticketId" name="ticketId" required />
    </div>
    <button type="submit" class="btn btn-primary">Validar</button>
</form>
